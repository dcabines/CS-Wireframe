@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CS Wireframe - @ViewBag.Title</title>
    @Styles.Render("~/bundles/Styles/base")
    @RenderSection("Head", false)
</head>
<body>
    <div class="container-fluid">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/Scripts/base")
    @RenderSection("Scripts", false)
</body>
</html>
