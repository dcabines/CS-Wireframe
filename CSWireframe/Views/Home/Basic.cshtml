<div class="row">
    <div class="col-xs-2 text-center">
        <h4>Active Products</h4>
        <ul class="list-unstyled" data-bind="foreach: active">
            <li>
                <span data-bind="text: name"></span> -
                <span data-bind="text: date"></span>
            </li>
        </ul>
    </div>

    <div class="col-xs-2 text-center">
        <h4>Canceled Products</h4>
        <ul class="list-unstyled" data-bind="foreach: canceled">
            <li>
                <span data-bind="text: name"></span> -
                <span data-bind="text: date"></span>
            </li>
        </ul>
    </div>

    <div class="col-xs-2 text-center">
        <h4>Zip Codes</h4>
        <ul class="list-unstyled" data-bind="foreach: zipCodes">
            <li data-bind="text: $data"></li>
        </ul>
    </div>

    <div class="col-xs-2">
        <h4>Video</h4>
        <a data-bind="attr: { href: video, target: '_blank' }, text: video"></a>
    </div>

    <div class="col-xs-2 col-xs-offset-2">
        <button type="button" class="btn btn-default pull-right">Edit</button>
    </div>
</div>

<div class="row">
    <div class="col-xs-12">
        <h4>About Me</h4>
        <p data-bind="text: aboutMe"></p>
    </div>
</div>

<div class="row">
    <div class="col-xs-12">
        <h4>My Services</h4>
        <p data-bind="text: myServices"></p>
    </div>
</div>

<div class="row">
    <div class="col-xs-12">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>FB Page ID</th>
                    <th>Name</th>
                    <th>Read Assoc</th>
                    <th>Profile</th>
                    <th>IDX</th>
                    <th>Feature</th>
                    <th>DS</th>
                    <th>Access Token</th>
                </tr>
            </thead>
            <tbody data-bind="foreach: fbPages">
                <tr>
                    <td data-bind="text: fbid"></td>
                    <td data-bind="text: name"></td>
                    <td data-bind="text: readAssoc"></td>
                    <td data-bind="text: profile"></td>
                    <td data-bind="text: idx"></td>
                    <td data-bind="text: feature"></td>
                    <td data-bind="text: ds"></td>
                    <td data-bind="text: accessToken"></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>